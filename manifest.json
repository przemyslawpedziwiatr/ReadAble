{
	"manifest_version": 2,
	"name" : "ReadAble reading enhancement",
	"description" : "Makes reading web articles a breeze!",
	"version" : "1.0",

	"browser_action" : {
		"default_icon" : "assets/icon.png",
		"default_popup" : "popup.html"
	},
	"permissions" : [
		"activeTab"
	],
	  "content_scripts": [
	    {
	      "matches": [
	        "<all_urls>"
	      ],
	      "js": ["sources/vendor/jquery.min.js", "readable.js"]
	    }
	  ]
}